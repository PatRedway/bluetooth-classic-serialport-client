{
  "name": "bluetooth-classic-serialport-client",
  "version": "0.1.4",
  "description": "Bluetooth classic serial port client for Node.js",
  "keywords": [
    "bluetooth",
    "serialport",
    "rfcomm",
    "linux",
    "windows"
  ],
  "author": "Lalaina Rahobisoa <lalaina.rahobisoa@gmail.com>",
  "homepage": "https://github.com/PatRedway/bluetooth-classic-serialport-client",
  "repository": {
    "type": "git",
    "url": "https://github.com/PatRedway/bluetooth-classic-serialport-client"
  },
  "gypfile": true,
  "main": "./index.js",
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "dependencies": {
    "aws-sdk": "^2.669.0",
    "nan": "2.14.1",
    "node-pre-gyp": "^0.14.0"
  },
  "binary": {
    "module_name": "bluetooth_classic_serialport_client",
    "module_path": "./lib/binding/",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz",
    "host": "https://bluetooth-classic-serialport-client.s3.amazonaws.com"
  },
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build",
    "test": "echo \"No test specified\""
  },
  "license": "MIT"
}
