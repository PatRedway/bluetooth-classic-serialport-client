{
  "name": "bluetooth-classic-serialport-client",
  "version": "0.1.3",
  "description": "Bluetooth classic serial port client for Node.js",
  "keywords": [
    "bluetooth",
    "serialport",
    "rfcomm",
    "linux",
    "windows"
  ],
  "author": "Lalaina Rahobisoa <lalaina.rahobisoa@aet.fr>",
  "homepage": "https://github.com/PatRedway/bluetooth-classic-serialport-client",
  "repository": {
    "type": "git",
    "url": "https://github.com/PatRedway/bluetooth-classic-serialport-client"
  },
  "gypfile": true,
  "main": "./index.js",
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "dependencies": {
    "nan": "2.14.1",
    "node-pre-gyp": "^0.14.0"
  },
  "binary": {
    "module_name": "bluetooth_classic_serialport_client",
    "module_path": "./lib/binding/",
    "host": "https://bitbucket.org/aet64/std-039-smartvox-config/downloads",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz"
  },
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build",
    "install-debug": "node-gyp configure build --debug"
  },
  "license": "MIT"
}
